amends "..."

deployments {
  ["app"] {
    spec {
      replicas = 1
      template {
        spec {
          containers {
            new {
              name = "app"
              image = "kajay0322/app"
              imagePullPolicy = "Always"
              ports {
                new {
                  containerPort = 8080
                }
              }
            }
          }
        }
      }
    }
  }
}
